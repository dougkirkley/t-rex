{
   "module_provider": {
       "name": "aws",
       "version": "~\u003e 3.2",
       "region": "us-east-1",
       "profile": "default"
   },
   "api_name": "apiname",
   "functions": [
       {
           "lambda_name": "getOrders",
           "lambda_runtime": "go1.x",
           "method": "GET",
           "root_path": "",
           "path": "orders",
           "authorization": "",
           "iam_policy": ""
       },
       {
           "lambda_name": "addOrders",
           "lambda_runtime": "go1.x",
           "method": "POST",
           "root_path": "",
           "path": "orders",
           "authorization": "",
           "iam_policy": ""
       },
       {
           "lambda_name": "getOrder",
           "lambda_runtime": "go1.x",
           "method": "GET",
           "root_path": "orders",
           "path": "{id}",
           "authorization": "",
           "iam_policy": ""
       }
   ]
}